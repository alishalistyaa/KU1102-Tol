                        color: 0,0,0,1
                        font_name: 'fonts/emulogic.ttf'
                        font_size: '64'

                    Button:
                        disabled: root.milihjenis_enabled
                        opacity: 1 if root.fasesatu else 0

                        background_normal: "photos/buttondown_scaled.png"
                        background_down: "photos/buttonuppressed_scaled.png"
