                    # Ini harusnya gif
                    # source: "photos/PINITB.png"

            AnchorLayout:
                # Background
                Image:
                    source: "photos/BG_Flat.png"
                    allow_stretch: True
                    keep_ratio: False
                
                # Fase Tiga
                FloatLayout:
                #:set x_nokiri 0.17
                #:set y_noatas 0.38
                #:set x_nokanan 0.57
                #:set y_nobawah 0.14

                #: set x_nosize 0.32
                #: set y_nosize 0.2


                    Button: 
                        disabled: not root.milihgandar_enabled
                        opacity: 1 if root.fasetiga else 0
                        background_normal: "photos/button1up.png"
                        background_down: "photos/buttonnodown.png"
                        allow_strect: True
                        keep_ratio: False
                        size_hint: 0.31, 0.19
                        pos_hint:{"x":x_nokiri, "y":0.45}
                        on_press:
                            opacity:0.5
                        on_release:
                            app.root.current = "masuktol"
                            root.manager.transition.direction = "left"
                            root.golongansatu()
                    
                    Button: 
                        disabled: not root.milihgandar_enabled
                        opacity: 1 if root.fasetiga else 0
                        background_normal: "photos/button2up.png"
                        background_down: "photos/buttonnodown.png"
                        allow_strect: True
                        keep_ratio: False
                        size_hint: x_nosize, y_nosize
                        pos_hint:{"x":x_nokanan, "y":y_noatas}
                        on_press:
                            opacity:0.5
                        on_release:
                            app.root.current = "masuktol"
                            root.manager.transition.direction = "left"
                            root.golongandua()

                    Button: 
                        disabled: not root.milihgandar_enabled
                        opacity: 1 if root.fasetiga else 0
                        background_normal: "photos/button3up.png"
                        background_down: "photos/buttonnodown.png"
                        allow_strect: True
                        keep_ratio: False
                        size_hint: 0.32, 0.2
                        pos_hint:{"x":x_nokiri, "y":0.22}
                        on_press:
                            opacity:0.5
                        on_release:
                            app.root.current = "masuktol"
                            root.manager.transition.direction = "left"
                            root.golongantiga()

                    Button: 
                        disabled: not root.milihgandar_enabled
                        opacity: 1 if root.fasetiga else 0
                        background_normal: "photos/button4up.png"
                        background_down: "photos/buttonnodown.png"
                        allow_strect: True
                        keep_ratio: False
                        size_hint: x_nosize, y_nosize
                        pos_hint:{"x":x_nokanan, "y":y_nobawah}
                        on_press:
                            opacity:0.5
                        on_release:
                            app.root.current = "masuktol"
                            root.manager.transition.direction = "left"
                            root.golonganempat()

                    Button: 
                        disabled: not root.milihgandar_enabled
                        opacity: 1 if root.fasetiga else 0
                        background_normal: "photos/button5up.png"
                        background_down: "photos/buttonnodown.png"
                        allow_strect: True
                        keep_ratio: False
                        size_hint: x_nosize, y_nosize
                        pos_hint:{"x":x_nokiri, "y":0.001}
                        on_press:
                            opacity:0.5
                        on_release:
                            app.root.current = "masuktol"
                            root.manager.transition.direction = "left"
                            root.golonganlima()
                # ----------------------------------------------

                # Fase Dua
                FloatLayout:
                    Button:
                        disabled: not root.milihjenis_enabled
                        opacity: 1 if root.fasedua else 0
                        background_normal: "photos/bisabu.png"
                        background_down: "photos/bisabudown.png"
                        allow_strect: True
                        keep_ratio: False
                        size_hint: 0.4,0.27
                        pos_hint:{"x":0.28, "y":0.33}
                        on_release: 
                            app.root.current = "masuktol"
                            root.manager.transition.direction = "left"
                            root.golongansatu()

