<Identifikasi>:
    name: "ketiga"
    FloatLayout:

        BoxLayout:
            orientation: "horizontal"
            size: root.width, root.height

            FloatLayout:
                Image:
                    source: "photos/BG_Half.png"
                    allow_stretch: True
                    keep_ratio: False
                    size: root.width, root.height
