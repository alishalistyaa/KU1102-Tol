                        font_size: '32'
                        on_release: 
                            if root.count > 2.1: root.on_satuselesai()
                            if root.count <= 2.1: app.root.current = "masuktol"  
                            if root.count <= 2.1: root.golongansatu()

        
        
        Image:
            source: "photos/dialougegandar.png" if root.fasetiga else "photos/dialougebox.png"
            allow_stretch: True
