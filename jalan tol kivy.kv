                        disabled: not root.milihjenis_enabled
                        opacity: 1 if root.fasedua else 0

                        background_normal: "photos/trukabu.png"
                        background_down: "photos/trukabudown.png"
                        allow_strect: True
                        keep_ratio: False
                        size_hint: 0.4,0.27
                        pos_hint:{"x":0.28, "y":0.03}
                        on_release: root.on_truk_duaselesai()
                
